                        "name": "minassdttlvalue",
                        "description": "The total assessed value from the local taxing authority",
                        "type": "string",
                        "required": false,
                        "in": "query",
                        "default": "27700"
                    },
                    {
                        "name": "maxassdttlvalue",
                        "description": "The total assessed value from the local taxing authority",
                        "type": "string",
                        "required": false,
                        "in": "query",
                        "default": "483600"
