                        "required": false,
                        "in": "query",
                        "default": "8370d93a17ba7fb07f115392bd1225d9"
                    },
                    {
                        "name": "startcalendardate",
                        "description": "The standardized date for search purposes",
                        "type": "string",
                        "format": "date",
                        "required": false,
                        "in": "query",
                        "default": "2017-04-01"
