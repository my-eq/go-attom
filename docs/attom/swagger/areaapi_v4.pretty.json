{
    "swagger": "2.0",
    "info": {
        "version": "4.0.0",
        "title": "",
        "description": ""
    },
    "host": "api.gateway.attomdata.com",
    "basePath": "/v4/area",
    "schemes": [
        "https"
    ],
    "tags": [
        {
            "name": "Area API"
        }
    ],
    "consumes": [
        "application/json",
        "application/xml"
    ],
    "produces": [
        "application/xml",
        "application/json"
    ],
    "paths": {
        "/boundary/detail": {
            "get": {
                "tags": [
                    "1. Area Boundary Detail"
                ],
                "summary": "Returns the boundaries of the geography requested.",
                "description": "This method allows you to pass a valid geocode (geoIdV4) and retrieve boundaries, centroid and area for the geography request",
                "operationId": "getBoundary",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "default": "application/json",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "geoIdV4",
                        "description": "Geocode requested",
                        "type": "string",
                        "required": true,
                        "default": "53205b2ae13a4203456e53ed9d56cfea",
                        "in": "query"
                    },
                    {
                        "name": "format",
                        "description": "Boundaries format",
                        "type": "string",
                        "required": false,
                        "in": "query",
                        "default": "geojson",
                        "enum": [
                            "geojson",
                            "wkt"
                        ]
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Geography information"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/hierarchy/lookup": {
            "get": {
                "tags": [
                    "2. Area Hierarchy Lookup"
                ],
                "summary": "Returns all boundaries a point falls within.",
                "description": "This method allows you to pass a latitude and longitude point and retrieve a list of geographies that point falls within. Required input is a valid WKT (Well Known Text) point.",
                "operationId": "getHierarchy",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "WKTString",
                        "description": "WKTString parameters",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "POINT(-71.5776 41.66895)"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of area hierarchy lookup"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/cbsa/lookup": {
            "get": {
                "tags": [
                    "3. Area CBSA Lookup"
                ],
                "summary": "Returns all CBSAs within a state.",
                "description": "This Core Based Statistical Area lookup allows you to find the CBSA within a given state. Required input is a state FIPS Code ID (e.g. New York is ST36).",
                "operationId": "getCBSA",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "StateId",
                        "description": "State Id",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "ST36"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of area CBSA lookup"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/county/lookup": {
            "get": {
                "tags": [
                    "4. Area County Lookup"
                ],
                "summary": "Returns all counties within a state.",
                "description": "This lookup returns all the Counties that are in a particular state. The two-digit FIPS code is required.",
                "operationId": "getCounty",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "StateId",
                        "description": "State Id",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "ST36"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of area county lookup"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/state/lookup": {
            "get": {
                "tags": [
                    "5. Area State Lookup"
                ],
                "summary": "Returns all states and their IDs.",
                "description": "This lookup returns a list of all states. State ID and name will be returned.",
                "operationId": "getState",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of area state lookup"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/geoid/lookup/": {
            "get": {
                "tags": [
                    "6. Area Geo ID Lookup"
                ],
                "summary": "Returns specific Geo IDs that exist within a specified Geo ID",
                "description": "This service allows you to enter a specified Geo ID and a Geo ID type. It will return the Geo IDs of the defined type that are related to the specified Geo ID.",
                "operationId": "getGeoID",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "geoId",
                        "description": "known Geo ID",
                        "type": "string",
                        "required": false,
                        "in": "query",
                        "default": "ST34"
                    },
                    {
                        "name": "geoIdV4",
                        "description": "known GeoIDV4",
                        "type": "string",
                        "required": false,
                        "in": "query"
                    },
                    {
                        "name": "GeoType",
                        "description": "Related Geo Type Returned",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "enum": [
                            "CI",
                            "CS",
                            "CO",
                            "DB",
                            "ND",
                            "PL",
                            "RS",
                            "ST",
                            "ZI",
                            "N1",
                            "N2",
                            "N3",
                            "N4"
                        ],
                        "default": "DB"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of Geo IDs"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/geoid/legacyLookup/": {
            "get": {
                "tags": [
                    "7. Area Geo ID Legacy Lookup"
                ],
                "summary": "Returns a translation between legacy codes and new geography identifiers (GeoIDV4)",
                "description": "This service allows you to find the new geography identifier (GeoIDV4) from a legacy geography code.",
                "operationId": "getGeoIDLegacy",
                "parameters": [
                    {
                        "name": "apikey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "geoId",
                        "description": "legacy geography code (separate multiple values with comma)",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "ZI92618,CO06073"
                    },
                    {
                        "name": "geoIdV4",
                        "description": "New geography identifier",
                        "type": "string",
                        "required": false,
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of legacy geography code and the corresponding geography identifiers (GeoIDV4)"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        }
    }
}
