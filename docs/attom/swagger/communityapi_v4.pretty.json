{
    "swagger": "2.0",
    "info": {
        "version": "4.0.0",
        "title": "",
        "description": ""
    },
    "host": "api.gateway.attomdata.com",
    "basePath": "/v4",
    "schemes": [
        "https"
    ],
    "tags": [
        {
            "name": "Community V4",
            "description": "Community-centric data for a specific geography type"
        }
    ],
    "consumes": [
        "application/json",
        "application/xml"
    ],
    "produces": [
        "application/xml",
        "application/json"
    ],
    "paths": {
        "/neighborhood/community": {
            "get": {
                "tags": [
                    "Community V4"
                ],
                "summary": "Community profile for a spcific geography (geoIdV4).",
                "description": "Community profile including air quailty, climate, crime, demographics, and natural disatsers for a specific geoggraphy (geoIdV4).",
                "operationId": "getCommunityAPIAreaFull",
                "parameters": [
                    {
                        "name": "ApiKey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "geoIdv4",
                        "description": "This is the GeoID to search",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "dd4ec3218a89807fc1c63dd7265cc1bc"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of community search geographies"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        },
        "/location/lookup": {
            "get": {
                "tags": [
                    "Community V4"
                ],
                "summary": "Geography search.",
                "description": "Text search of geographies. Used to identify applicatble geocodes (geoIdV4)",
                "operationId": "LocationLookup",
                "parameters": [
                    {
                        "name": "ApiKey",
                        "description": "Application Key",
                        "type": "string",
                        "in": "header",
                        "default": "",
                        "x-data-threescale-name": "user_keys"
                    },
                    {
                        "name": "accept",
                        "description": "application/xml or application/json (default)",
                        "type": "string",
                        "required": false,
                        "in": "header",
                        "enum": [
                            "application/xml",
                            "application/json"
                        ]
                    },
                    {
                        "name": "name",
                        "description": "This is the location name to search",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "Las Vegas"
                    },
                    {
                        "name": "geographyTypeAbbreviation",
                        "description": "Geography type abbreviation",
                        "type": "string",
                        "required": true,
                        "in": "query",
                        "default": "PL",
                        "enum": [
                            "CI",
                            "CO",
                            "CS",
                            "DB",
                            "N1",
                            "N2",
                            "N3",
                            "N4",
                            "PL",
                            "RS",
                            "ST",
                            "ZI"
                        ]
                    }
                ],
                "responses": {
                    "200": {
                        "description": "list of geographies"
                    },
                    "403": {
                        "description": "apikey invalid/missing"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        }
    }
}
